<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
        <link rel="stylesheet" href="./public/css/style.css">
        <link rel="stylesheet" href="./public/css/restmain.css">
            {# {{ encore_entry_link_tags('app') }} #}
        {% endblock %}

        {% block javascripts %}
         <script defer type="module" src="./public/js/insertproduits.js"></script>
        {# //  <script defer type="module" src="./public/js/main.js"></script>
        //  <script defer type="module" src="./public/js/panier.js"></script> #}
         <script defer type="module" src="./public/js/turn.js"></script>
            {# {{ encore_entry_script_tags('app') }} #}
        {% endblock %}
    </head>
    <body>
        {% include "template_parts/_header.html.twig" %}
        {% block body %}
        {% include "pages/main.html.twig" %} 
        {% endblock %}
        {% include "template_parts/_footer.html.twig" %}
    </body>
</html>
