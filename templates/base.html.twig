        {% endblock %}
        {% include "template_parts/_footer.html.twig" %}
    </body>
        {% block javascripts %}
        <script defer type="module" src="js/insertproduits.js"></script>
        <script defer type="module" src="js/main.js"></script>
        <script defer type="module" src="js/panier.js"></script> 
        <script defer type="module" src="js/turn.js"></script>
            {# {{ encore_entry_script_tags('app') }} #}
        {% endblock %}
